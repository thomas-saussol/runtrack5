{% extends 'base.html.twig' %}

{% block title %}Hello MyEntrepriseController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    {% for article in projet %}
       <a href="{{ path ("project_page", {'id': article.id}) }}" ><HEADER>{{ article.content|raw }}</HEADER></a>
        <h1>{{ article.title }}</h1>
       <img src="{{ article.image }}" >
        <h4>{{ article.createdAt|date('d:m:Y H:i:s') }}</h4>

    {% endfor %}
</div>
{% endblock %}
